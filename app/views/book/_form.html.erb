<%= form_with(model: book, local: true) do |f| %>
  <% if book.errors.any? %>
    <div id="error_explanation">
      <ul>
        <% book.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <li></li>
  <%= f.label :create_at, '日付' %><%= f.text_field :create_at %>,<%= f.label :category, 'カテゴリ' %><%= f.text_field :category %>, <%= f.label :title, 'タイトル' %><%= f.text_field :title %>
  <%= f.label :content, '内容' %><%= f.text_field :content%>




  <%= f.submit '投稿' %>
<% end %>
